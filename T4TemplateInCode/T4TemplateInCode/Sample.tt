<#@ template language="C#" #>
<#@ assembly name="System.Core" #>
<#@ import namespace="System.Linq" #>
<#@ import namespace="System.Text" #>
<#@ import namespace="System.Collections.Generic" #>

<h2>Customer Info</h2>  
<table>  
<# foreach (Customer item in Data)   
   { #>  
      <tr><td> <#= item.Name #> </td>  
          <td> <#= item.Address #> </td></tr>  
<# } // end of foreach  
#>  
</table>